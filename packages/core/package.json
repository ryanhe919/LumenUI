{
  "name": "@ryanhe919/lumen-ui",
  "version": "0.2.0",
  "description": "A modern React UI component library with glassmorphism design",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "glassmorphism",
    "design-system",
    "tailwindcss",
    "typescript"
  ],
  "author": "Yufan He",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heyufan/lumen-ui.git"
  },
  "bugs": {
    "url": "https://github.com/heyufan/lumen-ui/issues"
  },
  "homepage": "https://github.com/heyufan/lumen-ui#readme",
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^3.9.0"
  }
}
